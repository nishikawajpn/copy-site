            li(class="home-apps__link-item")
              a(href="#" class="home-apps__anchor")
                img(src="../images/badge_appStore.png" class="home-apps__app-image" alt="App Store からダウンロード" width="205" height="auto")
            li(class="home-apps__link-item")
              a(href="#" class="home-apps__anchor")
                img(src="../images/badge_googlePlay.png" class="home-apps__app-image" alt="Google Play で手に入れよう" width="205" height="auto")
